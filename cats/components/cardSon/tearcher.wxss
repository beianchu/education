/* components/cardSon/tearcher.wxss */
.box-big{
  width: 100%;
  box-sizing: border-box;
  padding: 1rpx 20rpx;
  height: 500rpx;
 
}
.box-title{
  width: 100%;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.box-title view:nth-child(1){
font-weight: 600;
font-size: 50rpx;
}
.box-title view:nth-child(2){
  font-size: 20rpx;
  color: gray;
}
.boxtearch1{
margin: 50rpx auto;
  display: flex;
  justify-content: space-around;
flex-wrap: wrap;
  width: 100%;
font-size: 20rpx;
}
.boxtearch{
  position: relative;
  width: 26%;
  padding: 20rpx;
  background-color: #eaeaea;
  border-radius: 10rpx;
}
.freebox{
  margin: 10rpx 0;
  width: 100%;
  background-color: #f15a24;
  color: white;
  border-radius:10rpx ;
  padding: 3rpx 0;
}
.img-header{
  position: absolute;
  top: -40rpx;left: 70rpx;
}
.title-top{
  margin-top: 50rpx;
}
.
element.style{
  z-index: 1;
  height: auto !important;
}