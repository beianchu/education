<!--components/banner-img/bannerlist.wxml-->
<view class="page-section page-section-spacing swiper" style="margin-top: 20rpx;">
      <swiper indicator-dots="{{indicatorDots}}"
        autoplay="true" interval="1000" duration="1000">
        <block>
         
          <swiper-item>
            <view class="swiper-item" style="width: 100%;height: 100%;display: flex;justify-content: center;">
              <image style="margin: 0 auto;width: 90%;height: 100%;text-align: center;border-radius: 10rpx;" src="https://lexuemiao.oss-cn-beijing.aliyuncs.com/uploads/image/2022o0z11tko1663914988.jpg"></image>
            </view>
          </swiper-item>
         
          
        </block>
      </swiper>
    </view>

